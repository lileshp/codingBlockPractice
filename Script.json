      "D1": {
        "Dialgoue_Character": "HANSEL",
        "Dialogue_sentiment": "Positive/Negative/Default",
        "Dialogue_Words": "Don't cry, trust me! I swear I'll take you home even if Father doesn t  come back for us!"
      },
      "D2": {
        "Dialgoue_Character": "HANSEL",
        "Dialogue_sentiment": "Positive/Negative/Default",
        "Dialogue_Words": "Now give me your hand!"
      },
      "D3": {
        "Dialgoue_Character": "HANSEL",
        "Dialogue_sentiment": "Positive/Negative/Default",
        "Dialogue_Words": "We'll get home safely, you'll see!"
      }
    },
    "Narration": {
